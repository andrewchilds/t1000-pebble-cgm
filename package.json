{
  "name": "T1000",
  "author": "Andrew Childs",
  "version": "1.0.0",
  "description": "Displays real-time Dexcom CGM glucose data on your Pebble watch. Features current glucose value with trend arrow, delta (rate of change), time since last reading, and a 90-minute history chart with threshold lines. Configurable units (mg/dL or mmol/L) and alert thresholds. Requires Dexcom CGM with Share enabled and Dexcom Share account credentials.",
  "private": true,
  "keywords": [
    "pebble-app"
  ],
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "sdkVersion": "3",
    "targetPlatforms": [
      "aplite"
    ],
    "enableMultiJS": true,
    "capabilities": [
      "configurable"
    ],
    "projectType": "native",
    "displayName": "T1000",
    "uuid": "e307c458-7520-48cb-9308-815eb50411e9",
    "watchapp": {
      "watchface": true,
      "hiddenApp": false,
      "onlyShownOnCommunication": false
    },
